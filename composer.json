{
    "name": "silverorange/mb_fpdf",
    "description": "Multi-byte aware adapter for FPDF",
    "type": "library",
    "keywords": [ "fpdf", "mbstring" ],
    "homepage": "https://github.com/silverorange/mb_fpdf",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Gauthier",
            "email": "mike@silverorange.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "setasign/fpdf": "^1.8.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.6.0"
    },
    "scripts": {
        "lint": [
            "phpcs --standard=PSR2 --extensions=php --colors src/"
        ]
    },
    "autoload": {
        "psr-4": {
            "silverorange\\MBFPDF\\" : "src/"
        }
    }
}
